---
import List, { type ListProps } from '@components/wrappers/List.astro'
import type { LinkProps } from './Link.astro'
import Link from './Link.astro'

type Props = Omit<LinkProps, 'label' | 'href'> &
  ListProps & {
    links: LinkProps[]
  }

const { links, ...restProps } = Astro.props
---

<List {...restProps}>
  {
    links?.map((link) => (
      <Link
        {...link}
        {...restProps}
      />
    ))
  }
</List>
