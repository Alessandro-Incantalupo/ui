---
// @ts-ignore
import npm from 'npm-stats-api'

const date = new Date()

let today = date.toJSON().slice(0, 10)
let yesterday = date.toJSON(date.setDate(date.getDate() - 1)).slice(0, 10)
---

{
  npm.stat('@fullui/ui', yesterday, today).then((res: any) => {
    return <Fragment set:html={res} />
  })
}
