{
  "name": "fulldev-ui",
  "version": "0.4.0",
  "description": "A component library built for Astro",
  "homepage": "https://ui.full.dev",
  "bugs": "https://github.com/fulldotdev/ui/issues",
  "license": "MIT",
  "author": "Fulldev <contact@full.dev> (https://full.dev)",
  "repository": "github:fulldotdev/ui",
  "keywords": [
    "withastro",
    "astro",
    "astro-component",
    "astro-component-library",
    "astro-ui-library",
    "astro-ui"
  ],
  "scripts": {
    "dev": "pnpm --filter docs dev",
    "check": "pnpm --filter docs check",
    "build": "pnpm --filter docs build",
    "preview": "pnpm --filter docs preview",
    "test": "pnpm check && build && preview"
  },
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*",
    "./[...page].astro": "./src/pages/[...page].astro",
    "./color": "./src/css/color.css",
    "./integration": "./src/integration/integration.ts",
    "./tailwind": "./src/integration/tailwind.ts",
    "./unocss": "./src/integration/unocss.ts"
  },
  "dependencies": {
    "@iconify-json/tabler": "^1.2.1",
    "astro-seo": "^0.8.4",
    "radash": "^12.1.0",
    "remeda": "2.12.0",
    "sass": "^1.78.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite-plugin-virtual": "^0.3.0",
    "@astrojs/sitemap": "^3.1.6",
    "astro": "^4.15.4",
    "@astrojs/mdx": "^3.1.5",
    "@astrolib/seo": "1.0.0-beta.5",
    "@radix-ui/colors": "^3.0.0",
    "colorjs.io": "^0.5.2",
    "deepmerge": "^4.3.1",
    "bezier-easing": "^2.1.0",
    "astro-live-code": "^0.0.3"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.3",
    "prettier": "3.3.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-astro-organize-imports": "^0.4.9",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-organize-imports": "^4.0.0"
  }
}