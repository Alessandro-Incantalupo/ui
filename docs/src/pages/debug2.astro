---
import {
  getCollectionCascade,
  transformCasing,
  transformLayouts,
  transformReferences,
  transformSelfs,
} from '@fullui/ui'

let cascade: any = await getCollectionCascade(['pages', 'layouts', 'sections'])
console.log('cascade', cascade.pages.index)

cascade = transformReferences(cascade)
console.log('references', cascade.pages.index)

cascade = transformLayouts(cascade)
console.log('layouts', cascade.pages.index)

cascade = transformSelfs(cascade)
console.log('selfs', cascade.pages.index)

cascade = transformCasing(cascade)
console.log('casing', cascade.pages.index)
---
