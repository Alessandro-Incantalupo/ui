---
import { getCollectionCascade, transformLayouts } from '@fullui/ui'
import { getEntry } from 'astro:content'

const cascade = await getCollectionCascade([
  'pages',
  'layouts',
  'aanbod',
  'team',
])
const home = await getEntry('pages', 'index')
const transformed = transformLayouts(home.data, cascade)
