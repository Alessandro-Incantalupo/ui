---
import { Button, Header as FullHeader, Heading, Nav } from '@fullui/ui'
---

<FullHeader
  density="compact"
  size="md"
  position="relative"
>
  <Heading style="font-size: 24px"> Fullui </Heading>
  <Nav>
    <Button
      color="base"
      title="Linkedin"
      icon={{ name: 'brand-linkedin' }}
      href="https://linkedin.com/in/silveltman/"
      target="_blank"
    />
    <Button
      color="base"
      title="X / Twitter"
      icon={{ name: 'brand-x' }}
      href="https://x.com/silveltm"
      target="_blank"
    />
    <Button
      color="secondary"
      html="Github"
      icon={{ name: 'brand-github' }}
      href="https://github.com/fullui/ui"
      target="_blank"
    />
    <Button
      color="primary"
      html="Docs"
      href="/overview/introduction/"
    />
  </Nav>
</FullHeader>
