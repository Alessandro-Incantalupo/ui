---
import { Button, Header as FullHeader, Group, Heading, Link } from 'fulldev-ui'
---

<FullHeader
  density="compact"
  size="lg"
  x="spread"
  position="relative"
  style={'border-bottom: 1px solid #262626;'}
>
  <Heading size="lg"> Fulldev UI</Heading>

  <Group>
    <Link
      text="features"
      href="#features"
    />
    <Link
      text="documentation"
      href="/overview/introduction/"
    />
  </Group>

  <Group>
    <Button
      variant="ghost"
      title="Linkedin"
      icon={{ name: 'brand-linkedin' }}
      href="https://linkedin.com/in/silveltman/"
      target="_blank"
    />
    <Button
      variant="ghost"
      title="X / Twitter"
      icon={{ name: 'brand-x' }}
      href="https://x.com/silveltm"
      target="_blank"
    />
    <Button
      variant="ghost"
      title="Discord"
      icon={{ name: 'brand-discord' }}
      href="https://discord.gg/vXZqMbadm8"
      target="_blank"
    />
  </Group>
</FullHeader>
