---
import { Button, Header as FullHeader, Group, Heading } from 'fulldev-ui'
---

<FullHeader
  density="compact"
  size="md"
  position="relative"
>
  <Heading size="lg"> Fulldev UI </Heading>
  <Group>
    <Button
      variant="ghost"
      title="X / Twitter"
      icon={{ name: 'brand-x' }}
      href="https://x.com/silveltm"
      target="_blank"
    />
    <Button
      variant="ghost"
      title="Discord"
      icon={{ name: 'brand-discord' }}
      href="https://discord.gg/vXZqMbadm8"
      target="_blank"
    />
    <Button
      variant="secondary"
      html="Github"
      icon={{ name: 'brand-github' }}
      href="https://github.com/fulldotdev/ui"
      target="_blank"
    />
    <Button
      variant="primary"
      html="Docs"
      href="/overview/introduction/"
    />
  </Group>
</FullHeader>
