---
import { Button, Header as FullHeader, Group, Heading } from 'fulldev-ui'
---

<FullHeader
  compact
  size="md"
  position="relative"
>
  <Heading
    as="a"
    href="/"
    size="lg"
  >
    Fulldev UI
  </Heading>
  <Group>
    <Button
      variant="ghost"
      title="X / Twitter"
      icon={{ name: 'brand-x' }}
      href="https://x.com/silveltm"
      target="_blank"
    />
    <Button
      variant="ghost"
      title="Discord"
      icon={{ name: 'brand-discord' }}
      href="https://discord.gg/vXZqMbadm8"
      target="_blank"
    />
    <Button
      variant="soft"
      html="Github"
      icon={{ name: 'brand-github' }}
      href="https://github.com/fulldotdev/ui"
      target="_blank"
    />
    <Button
      html="Docs"
      href="/overview/introduction/"
    />
  </Group>
</FullHeader>
