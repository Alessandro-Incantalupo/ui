---
import { Button, Header as FullHeader, Heading, Nav } from 'fulldev-ui'
---

<FullHeader
  density="compact"
  size="md"
  x="spread"
  position="relative"
>
  <Heading
    size="xl"
    as="a"
    href="/"
  >
    Fulldev UI</Heading
  >
  <Nav>
    <Button
      variant="ghost"
      title="Linkedin"
      icon={{ name: 'brand-linkedin' }}
      href="https://linkedin.com/in/silveltman/"
      target="_blank"
    />
    <Button
      variant="ghost"
      title="X / Twitter"
      icon={{ name: 'brand-x' }}
      href="https://x.com/silveltm"
      target="_blank"
    />
    <Button
      variant="ghost"
      title="Discord"
      icon={{ name: 'brand-discord' }}
      href="https://discord.gg/vXZqMbadm8"
      target="_blank"
    />
    <Button
      variant="secondary"
      html="Github"
      icon={{ name: 'brand-github' }}
      href="https://github.com/fulldotdev/ui"
      target="_blank"
    />
    <Button
      variant="primary"
      html="Docs"
      href="/overview/introduction/"
    />
  </Nav>
</FullHeader>
