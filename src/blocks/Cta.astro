---
import MasterSection from 'fulldev-ui/components/MasterSection.astro'

const {
  structure = 'column',
  size = 'lg',
  variant = 'subtle',
  align = 'center',
  frame = 'panel',
  ...rest
} = Astro.props
---

<MasterSection
  class:list={'cta'}
  {structure}
  {size}
  {variant}
  {align}
  {frame}
  {...rest}
/>
