---
import { Layout, Renderer } from '@silveltman/fullui/src/components'
import type { CollectionEntry } from 'astro:content'

interface Props {
  entry: CollectionEntry<'services'>
}

const { entry } = Astro.props
---

<Layout
  {...entry.data}
  class="services-layout"
>
  <Renderer blocks={entry.data.blocks} />
  <slot />
</Layout>
