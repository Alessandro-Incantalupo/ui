---
import { Blocks, Layout } from '@silveltman/fullui/src/components'
import type { CollectionEntry } from 'astro:content'

interface Props {
  entry: CollectionEntry<'jobs'>
}

const { entry } = Astro.props
---

<Layout
  {...entry.data}
  class="jobs-layout"
>
  <Blocks {...entry.data} />
  <slot />
</Layout>
