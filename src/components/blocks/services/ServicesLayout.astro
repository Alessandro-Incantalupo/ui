---
import LayoutAction from '@layouts/LayoutAction.astro'
import type { CollectionEntry } from 'astro:content'
import BlockListRenderer from '../../misc/BlockListRenderer.astro'

interface Props {
  entry: CollectionEntry<'services'>
}

const { data } = Astro.props.entry
---

<LayoutAction seo={data.seo}>
  <BlockListRenderer sections={data.sections} />
</LayoutAction>
