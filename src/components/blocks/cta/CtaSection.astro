---
import type { ComponentProps } from 'astro/types'
import Section from '../../elements/section/Section.astro'

export interface Props
  extends Pick<
    ComponentProps<typeof Section>,
    | 'layout'
    | 'scheme'
    | 'area'
    | 'impact'
    | 'tagline'
    | 'heading'
    | 'text'
    | 'buttons'
  > {}

const { props } = Astro
---

<Section
  class="cta-section flow"
  {...props}
/>
