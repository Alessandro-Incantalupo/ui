---
import type { ComponentProps } from 'astro/types'
import { LogoImage, LogoRoot } from 'src'

interface Props
  extends ComponentProps<typeof LogoRoot>,
    ComponentProps<typeof LogoImage> {}

const { props } = Astro
---

<LogoRoot
  class="logo"
  {...props}
>
  <LogoImage {...props} />
  <slot />
</LogoRoot>
