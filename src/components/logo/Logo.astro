---
import type { ComponentProps } from 'astro/types'
import { LogoImage, LogoRoot } from '.'
import { Text } from '../text'

interface Props
  extends ComponentProps<typeof LogoRoot>,
    ComponentProps<typeof LogoImage> {}

const { props } = Astro
---

<LogoRoot {...props}>
  <LogoImage {...props} />
  <Text {...props} />
  <slot />
</LogoRoot>
