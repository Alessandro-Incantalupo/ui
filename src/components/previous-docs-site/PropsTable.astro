---
import Table from 'fulldev-ui/components/table/Table.astro'

const { props } = Astro.props
---

<Table
  class:list={'not-content'}
  style={{ 'border-radius': '0px' }}
  size="md"
  header={['Prop', 'Type', 'Default']}
  body={props.map((item: any) => [
    item.name,
    item.type,
    item.default === undefined ? '-' : `${item.default}`,
  ])}
/>
