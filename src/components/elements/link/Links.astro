---
import type { Props as LinkProps } from './Link.astro'
import Link from './Link.astro'

interface Props extends Omit<LinkProps, 'label' | 'href'> {
  links: LinkProps[]
}

const { links, ...restProps } = Astro.props
---

{
  links?.map((link) => (
    <Link
      {...link}
      {...restProps}
    />
  ))
}
