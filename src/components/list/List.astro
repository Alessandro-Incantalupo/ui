---
import { ComponentProps } from 'astro/types'
import { ListRoot } from '.'
import Icon from '../icon/Icon.astro'
import Text from '../text/Text.astro'

interface Props
  extends ComponentProps<typeof ListRoot>,
    ComponentProps<typeof Icon>,
    ComponentProps<typeof Text> {}

const { props } = Astro
---

<ListRoot>
  <Icon {...props} />
  <Text {...props} />
</ListRoot>
